/* pages/seeworks/index.wxss */
.tittle{
  width: 90%;
  height: 300rpx;
  color: #dbdbdb;
  font-size: 50rpx;
  margin: 0 auto;
  text-align: center;
  margin-top: 80rpx;
  margin-bottom: -120rpx;
}
.img{
  width: 100%;
  height: 100%;
}
.left{
  float: left;
  width: 10%;
  height: 30%;
}
.right{
  float: right;
  width: 10%;
  height: 30%;
}

.finish{
  position: relative;
  margin: 0 auto;
  width: 90%;
  height: 200rpx;
}
.circle{
  background-color: #57606f;
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 0;
  left: 0;
  border-radius: 50%;
}
.can{
  width: 100rpx;
  height: 80rpx;
  position: absolute;
  left: 30rpx;
  top: 15rpx;
  /* #dbdbdb */
  border-top: 1px solid #dbdbdb;
  border-right: 1px solid #dbdbdb;
}
.toright{
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  top: 95rpx;
  left: 130rpx;
  border-top: 1px solid #dbdbdb;
}

.time,
.spendtime{
  font-size: 22rpx;
}

.time{
  position: absolute;
  top: -30rpx;
}
.spendtime{
  position: absolute;
  top: 0rpx;
}

.plan{
  position: absolute;
  left: 155rpx;
  top: 95rpx;
  width: 50%;
  height: 50%;
  border-radius: 13rpx;
  font-weight: 900;
  color: #d1d8e0;
  text-align: center;
  background-color: #45aaf2;
  font-size: 31rpx;
  box-shadow: 13rpx 13rpx 5rpx #57606f;
}

.bg{
  width: 90%;
  height: 50%;
  margin: 0 auto;
  margin-top: 200rpx;
}
.img{
  width: 60%;
  height: 60%;
  margin-left: 124rpx;
}
.unfi{
  margin-left: 55rpx;
  color: #d1d8e0;
  font-size: 50rpx;
  font-weight: 700;
}